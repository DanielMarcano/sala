{
    "extends": "airbnb-base",
    "rules": {
      "no-undef": [0],
      "arrow-body-style": [0],
      "no-console": [0],
      "eol-last": [0],
      "no-underscore-dangle": [0],
      "func-names": [0],
      "function-paren-newline": [0],
      "no-unused-vars": ["error", {
        "args": "none"
      }],
      "consistent-return": [0],
      "no-else-return": [0]
    }
}
